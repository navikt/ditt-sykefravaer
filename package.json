        "eslint-config-next": "^13.2.4",
        "eslint-plugin-cypress": "^2.12.1",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "jest-react-hooks-shallow": "^1.5.1",
        "next": "^12.3.1",
        "prettier": "^2.8.4",
        "react-test-renderer": "^18.2.0",
        "start-server-and-test": "^2.0.0",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.5",
        "yet-another-fetch-mock": "^4.1.0"
    },
    "scripts": {
        "dev": "MOCK_BACKEND=true next dev -p 8080 | pino-pretty",
        "dev-ingen-dekorator": "MOCK_BACKEND=true NO_DECORATOR=true npm run dev | pino-pretty",
        "start": "next start -p 8080",
        "start-ingen-dekorator": "NO_DECORATOR=true npm start",
        "build": "next build",
        "test": "jest --watch",
        "test:ci": "jest --ci",
        "cy:open": "cypress open --e2e",
        "e2e": "start-test dev-ingen-dekorator http://localhost:8080/syk/sykefravaer cy:open",
        "prettier:write": "prettier --write .",
        "prettier:check": "prettier --check .",
        "lint": "eslint --ext=ts,tsx src cypress",
        "lint:fix": "eslint --ext=ts,tsx src cypress --fix",
        "format": "npm run prettier:write && npm run lint:fix"
    },
    "eslintConfig": {
        "extends": [
            "@navikt/eslint-config-teamsykmelding",
            "next/core-web-vitals",
            "plugin:cypress/recommended"
        ],
        "rules": {
            "@typescript-eslint/explicit-function-return-type": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/no-non-null-assertion": "off",
            "@next/next/no-img-element": "off"
        }
    },
    "prettier": "@navikt/eslint-config-teamsykmelding/prettier"
}
