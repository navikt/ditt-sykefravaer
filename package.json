        "next-logger": "^3.0.1",
        "openid-client": "^5.4.0",
        "pino-pretty": "^9.4.0",
        "prom-client": "^14.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-modal": "^3.16.1",
        "react-query": "^3.39.3",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "^5.17.0",
        "@navikt/eslint-config-teamsykmelding": "^1.4.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^29.5.0",
        "@types/node": "^18.13.0",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
